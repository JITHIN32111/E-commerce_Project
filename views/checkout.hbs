                    </div><!-- End .checkout-discount -->
                    <form id="checkout-form">
                        <div class="row">
                            <div class="col-lg-9">
                                <h2 class="checkout-title">Billing Details</h2><!-- End .checkout-title -->
                                {{#each address}}
                                <tr>
                                    <td>
                                        <input checked value="{{this._id}}" type="radio" name="addressId">
                                        <span
                                            style="display: inline-block; height: 100px; width: 350px; border: 1px solid rgba(3, 91, 255, 0.73); margin-bottom: 20px; padding: 20px; border-radius: 6px;">
                                            {{!-- <div class="row"> --}}
                                                <div> {{this.firstname}} {{this.lastname}}</div>
                                                {{this.companyname}}
                                                {{this.towncity}}
                                                {{this.state}},
                                                {{this.country}},
                                                {{this.pincode}},

                                                <div class="mb-2"> {{this.email}}, {{this.phonenumber}}</div>
                                                


                                                {{!--
                                            </div> --}}
                                        </span>
                                    </td> <br>

                                </tr>
                                {{/each}}
                               <input type="text" name="userId" id="" value="{{user._id}}" hidden>

                                <div class="col-md-6">
                                    <p class="mt-3">Click here for add new address</p>
                                    {{!-- <button class="btn btn-outline-primary-2 btn-order btn-block mt-2"
