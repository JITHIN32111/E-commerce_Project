                                    --}}
                                    <a href="/address" class="btn btn-outline-info text-dark mt-3">add address</a>
                                </div>

                            </div><!-- End .col-lg-9 -->

                            {{!-- Next box --}}
                            <aside class="col-lg-3 mt-1">
                                <div class="summary" >
                                    <h3 class="summary-title">Your Order</h3><!-- End .summary-title -->
                                     <h4>
                                       Sub Total: {{total}}
                                     </h4>
                                    <table class="table table-summary">
                                        <thead>
                                            <tr>
                                                <th>Product</th>
                                                <th>price</th>
                                            </tr>
                                        </thead>

                                        <tbody>
                                            {{#each products}}
                                            <tr>
                                                <td class="text-dark"><a href="#">{{this.product.name}}</a></td>
                                                <td class="text-dark">{{this.product.price}}</td>

                                            </tr>

                                            {{/each}}

                                            <tr>
                                                <td>Shipping:</td>
                                                <td>Free shipping</td>
                                            </tr>
                                            <tr class="summary-total">
                                                <td >Total:</td>
                                                <td id="showTotal">{{total}}</td>
                                                
                                                <input type="text" id="total" value="{{total}}"  hidden>
                                                <input type="text" id="newTotal" value="{{total}}" name="newTotal" hidden>
                                            </tr><!-- End .summary-total -->
                                        </tbody>
                                    </table><!-- End .table table-summary -->

                                    <div class="accordion-summary" id="accordion-payment">

                                        <div class="payment">
                                <input type="text" name="ogTotal" value="{{total}}" hidden>
    
                                       <input type="text"   name="coupenName" id="cname" hidden>
                                       <input type="text" name="coupenPercentage" id="cpercentage" hidden>
                                    <input type="text" name="walletAount" value="{{amount}}" id="walletAmount" hidden>



                                            <p>Payment Method</p>
                                            <label for="" class="radio-inline">
                                                <input type="radio" name="payment-method" value="Wallet" onclick="wallet()" id="walletButton"  checked>Wallet
                                                <br>
                                                <span id="walletMsg"></span>
                                            </label>
                                            <hr>
                                            <label for="" class="radio-inline">
                                                <input type="radio" name="payment-method" value="COD"   checked>COD
                                            </label>
                                            <hr>
                                            <label for="" class="radio-inline">
                                                <input type="radio" name="payment-method" value="Razorpay"
                                                    checked>Razorpay
                                            </label>
                                            <br>
                                            <br>
                                            <label for="" class="radio-inline">
                                                <input type="radio" name="payment-method" value="paypal" checked>paypal
                                            </label>
                                        </div>

                                    </div><!-- End .accordion -->
                                    {{#if address}}
                                    <button type="submit" class="btn btn-outline-primary-2 btn-order btn-block">place
                                        order</button>
                                        {{else}}
                                        {{/if}}
                                </div><!-- End .summary -->
                            </aside><!-- End .col-lg-3 -->
                        </div><!-- End .row -->
                    </form>
                </div><!-- End .container -->
